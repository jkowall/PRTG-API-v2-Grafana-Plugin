{
  "name": "prtg-api-v2-datasource",
  "version": "1.0.0",
  "description": "Grafana datasource plugin for PRTG API v2",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.js --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.js --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec playwright test",
    "e2e:update": "npm exec playwright test --update-snapshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Your Organization",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@emotion/css": "^11.1.0",
    "@emotion/react": "^11.4.1",
    "@grafana/data": "*",
    "@grafana/runtime": "*",
    "@grafana/ui": "*",
    "lodash": "^4.17.21",
    "react": "*",
    "react-dom": "*",
    "rxjs": "^7.8.0",
    "tslib": "2.4.0"
  },
  "peerDependencies": {
    "@grafana/data": "*",
    "@grafana/runtime": "*",
    "@grafana/ui": "*",
    "react": "*",
    "react-dom": "*"
  },
  "packageManager": "npm@8.19.4"
}
